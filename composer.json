{
  "name": "mts7/bitwise-operations",
  "description": "Bitiwse operations examples",
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "icanhazstring/composer-unused": "^0.8.4",
    "infection/infection": "^0.26.15",
    "phing/phing": "^2.17",
    "phpmd/phpmd": "^2.13",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5",
    "roave/psalm-html-output": "^1.0",
    "slevomat/coding-standard": "^8.5",
    "squizlabs/php_codesniffer": "^3.7",
    "vimeo/psalm": "^4.27"
  },
  "autoload": {
    "psr-4": {
      "BitOps\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BitOps\\Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true
    }
  }
}

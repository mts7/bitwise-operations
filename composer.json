{
  "name": "mts7/bitwise-operations",
  "description": "Bitiwse operations examples",
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mts7/php-code-quality": "^0.1.4",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "BitOps\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BitOps\\Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true
    }
  }
}

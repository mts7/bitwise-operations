{
  "name": "vendor_name/bitwise-operations",
  "description": "Bitiwse operation examples",
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.27",
    "phpstan/phpstan": "^1.8",
    "phpmd/phpmd": "^2.13",
    "phing/phing": "^2.17",
    "squizlabs/php_codesniffer": "^3.7",
    "slevomat/coding-standard": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "BitOps\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BitOps\\Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
